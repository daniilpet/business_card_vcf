{
    "name": "econnect/vcard",
    "description": "Удобный класс для создания файлов vCard. В этой версии используется стандарт 3-ей версии для совместимости со многими устройствами.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "laravel",
        "vcard",
        "vcf",
        "contact",
        "contacts"
    ],
    "authors": [
        {
            "name": "Tom Witkowski",
            "email": "gummibeer@astrotomic.info",
            "homepage": "https://gummibeer.de",
            "role": "Первоначальный разработчик"
        },
        {
            "name": "Даниил Петрянкин",
            "email": "petryankin@econnect.one",
            "homepage": "https://dltd.bitrix24.ru/company/personal/user/10/",
            "role": "Разработчик"
        }
    ],
    "homepage": "https://github.com/daniilpet/business_card_vcf",
    "require": {
        "php": "^8.3",
        "astrotomic/php-conditional-proxy": "^0.2.1",
        "illuminate/contracts": "^9.0 || ^10.0 || ^11.0",
        "illuminate/http": "^9.0 || ^10.0 || ^11.0",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.0",
        "phpunit/phpunit": "^9.3 || ^10.0",
        "spatie/phpunit-snapshot-assertions": "^4.2 || ^5.1"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Econnect\\Vcard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Econnect\\Vcard\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "fix": "@php vendor/bin/pint"
    }
}